#include "s21_string.h"

#test s21_strcpy_test
char test_1[] = "123456";
char test_2[] = "      ";
char test_3[] = "123\056";
char test_4[] = "copy\0 don't copy";
char test_5[] = "";
char test_6[] = "School 21 it's a best school of programming";
char test_7[] = "123\156";
char test_8[] = "1234567";

ck_assert_str_eq(strcpy(test_1,test_2), s21_strcpy(test_1,test_2));
ck_assert_str_eq(strcpy(test_2,test_3), s21_strcpy(test_2,test_3));
ck_assert_str_eq(strcpy(test_4,test_5), s21_strcpy(test_4,test_5));
ck_assert_str_eq(strcpy(test_5,test_4), s21_strcpy(test_5,test_4));
ck_assert_str_eq(strcpy(test_6,test_5), s21_strcpy(test_6,test_5));
ck_assert_str_eq(strcpy(test_8,test_7), s21_strcpy(test_8,test_7));

#test s21_strncpy_test
char test_1[] = "123456";
char test_2[7] = "";
char test_3[] = "123\056";
char test_4[] = "copy\0 don't copy";
char test_5[17] = "";
char test_7[] = "123\156";
char test_8[] = "1234567";
char src1[] = "test test";
char dest1[] = "test_1";

ck_assert_str_eq(strncpy(test_2,test_1,3), s21_strncpy(test_2,test_1,3));
ck_assert_str_eq(strncpy(test_2,test_3,5), s21_strncpy(test_1,test_2,5));
ck_assert_str_eq(strncpy(test_5,test_4,4), s21_strncpy(test_5,test_4,4));
ck_assert_str_eq(strncpy(test_8,test_7,7), s21_strncpy(test_8,test_7,7));
ck_assert_str_eq(strncpy(test_8,test_7,4), s21_strncpy(test_8,test_7,4));
ck_assert_str_eq(strncpy(test_7,test_8,5), s21_strncpy(test_7,test_8,5));
ck_assert_str_eq(strncpy(src1,dest1,7), s21_strncpy(src1,dest1,7));
ck_assert_str_eq(strncpy(src1,dest1,6), s21_strncpy(src1,dest1,6));
ck_assert_str_eq(strncpy(src1,dest1,8), s21_strncpy(src1,dest1,8));

#test s21_strcmp_test
char test_1[] = "123456";
char test_2[] = "123456";
char test_3[] = "223456";
char test_4[] = "223456_";
char test_5[] = "223456";
char test_6[] = "223656";
char test_7[] = "   ";
char test_8[] = "\\\\HH";

ck_assert_uint_eq(strcmp(test_1,test_2) == 0, s21_strcmp(test_1,test_2) == 0);
ck_assert_uint_eq(strcmp(test_2,test_3) < 0, s21_strcmp(test_2,test_3) < 0);
ck_assert_uint_eq(strcmp(test_3,test_2) > 0, s21_strcmp(test_3,test_2) > 0);
ck_assert_uint_eq(strcmp(test_3,test_4) < 0, s21_strcmp(test_3,test_4) < 0);
ck_assert_uint_eq(strcmp(test_4,test_3) > 0, s21_strcmp(test_4,test_3) > 0);
ck_assert_uint_eq(strcmp(test_6,test_5) > 0, s21_strcmp(test_6,test_5) > 0);
ck_assert_uint_eq(strcmp(test_5,test_6) < 0, s21_strcmp(test_5,test_6) < 0);
ck_assert_uint_eq(strcmp(test_7,test_8) < 0, s21_strcmp(test_7,test_8) < 0);

#test s21_strncmp_test

char test_1[] = "123456";
char test_2[] = "123456";
char test_7[] = "   ";
char test_8[] = "\\\\HH";

ck_assert_uint_eq(strncmp(test_1,test_2,3), s21_strncmp(test_1,test_2,3));
ck_assert_uint_eq(strncmp(test_7,test_8,2) > 0, s21_strncmp(test_7,test_8,2) > 0);
ck_assert_uint_eq(strncmp(test_8,test_7,2) < 0, s21_strncmp(test_8,test_7,2) < 0);

